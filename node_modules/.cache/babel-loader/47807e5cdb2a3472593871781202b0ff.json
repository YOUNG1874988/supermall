AqC;AAAA;;AACnC,OAAKC,IAAL,GAAYH,QAAQ,CAACG,IAArB;AACA,OAAKC,KAAL,GAAaJ,QAAQ,CAACI,KAAtB;AACA,OAAKC,QAAL,GAAgBL,QAAQ,CAACK,QAAzB;AACA,OAAKC,QAAL,GAAgBN,QAAQ,CAACO,YAAzB;AACA,OAAKN,OAAL,GAAeA,OAAf;AACA,OAAKC,OAAL,GAAeA,OAAf;AACD,CARH,C,CAYA;;AACA","sourcesContent":["//1.导入基本请求配置函数，基本配置中的url可以到达服务器地址，但是没有具体指明请求哪个数据\nimport {request,requestRecommend} from './request'\nimport {requestgoods} from './requestgoods'\nimport {requestdetail} from './requestgoodsdetail'\n//2.配置请求首页多个数据的函数\nexport function getHomeMultidata(){\n  return request({\n    url:'/home/multidata'\n  })\n}\n//3.配置请求首页商品信息的函数\nexport function getHomeGoods(type,page){\n  return requestgoods({\n    url:'/home/data',\n    params:{\n      type,\n      page,\n    }\n  })\n}\n//4.配置请求商品详情页数据的函数\nexport function  getGoodsDetail(iid) {\n return requestdetail({\n   url:'/detail',\n   params:{\n     iid\n   }\n })\n}\n//5.配置请求商品详情页推荐数据的请求\nexport function getRecommend(){\n  return requestRecommend({\n    url:'/recommend'\n  })\n}\n//由于请求到的数据是分散的，故需要为商品详细信息创建类，方便拿取\nexport class Goods{\n  constructor(itemInfo,columns,service){\n    this.desc = itemInfo.desc;\n    this.price = itemInfo.price;\n    this.oldprice = itemInfo.oldprice;\n    this.discount = itemInfo.discountDesc;\n    this.columns = columns;\n    this.service = service\n  }\n}\n\n\n//api变更后原商品请求废弃\n/*export function getHomeGoods(type,page){\n  return request({\n    url:'/home/data',\n    params:{\n      type,\n      page\n    }\n  })\n}*/\n"]}]}